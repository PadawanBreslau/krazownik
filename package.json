    "react-ga": "^2.4.1",
    "react-gradient-progress": "^1.0.3",
    "react-gtm-module": "^2.0.8",
    "react-helmet": "5.2.0",
    "react-image-gallery": "^1.0.7",
    "react-input-range": "^1.3.0",
